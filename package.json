{
	"name": "charmed-icons",
	"displayName": "Charmed Icons",
	"description": "üíù Charming icon set for Roblox codebases",
	"version": "0.2.0",
	"publisher": "littensy",
	"icon": "assets/icon.png",
	"license": "MIT",
	"type": "module",
	"packageManager": "pnpm@9.4.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/littensy/charmed-icons"
	},
	"engines": {
		"vscode": "^1.88.0"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"iconThemes": [
			{
				"id": "charmed-icons",
				"label": "Charmed",
				"path": "./themes/charmed-icon-theme.json"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Charmed Icons",
			"properties": {
				"charmed-icons.hidesExplorerArrows": {
					"type": "boolean",
					"default": false,
					"description": "Hide arrow icons in the explorer section."
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint ."
	},
	"devDependencies": {
		"@eslint/js": "^9.5.0",
		"@vscode/vsce": "^2.28.0",
		"eslint": "^9.5.0",
		"eslint-plugin-json": "^4.0.0"
	}
}
